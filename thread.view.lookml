- view: thread
  sql_table_name: public.thread
  fields:

  - dimension: threadid
    primary_key: true
    sql: ${TABLE}.threadid

  - dimension_group: createdat
    type: time
    timeframes: [time, date, week, month]
    sql: ${TABLE}.createdat

  - dimension: threadtype
    type: number
    sql: ${TABLE}.threadtype

  - measure: count
    type: count
    drill_fields: detail*


